            "text": "/* cookie.css */\n@import url('variables.css');\n\n/* Overlay sombre pour la modale cookie */\n#cookie-overlay {\n  position: fixed !important;\n  top: 0; left: 0; right: 0; bottom: 0;\n  background: rgba(0,0,0,0.45);\n  z-index: 9998 !important;\n  display: block !important;\n}\n#cookie-overlay.hidden {\n  display: none !important;\n}\n\n/* Modale centrée */\n#cookie-modal {\n  position: fixed !important;\n  top: 50% !important;\n  left: 50% !important;\n  transform: translate(-50%, -50%) !important;\n  z-index: 9999 !important;\n  background: var(--primary-color, #17497a);\n  color: var(--light-text, #fff);\n  max-width: 340px;\n  width: 90vw;\n  padding: 28px 20px 20px 20px;\n  border-radius: 14px;\n  box-shadow: 0 8px 32px rgba(0,0,0,0.16);\n  display: flex !important;\n  flex-direction: column;\n  align-items: center;\n  text-align: center;\n  gap: 22px;\n  font-size: 1rem;\n  font-family: inherit;\n  animation: fadeInCookie 0.5s;\n}\n#cookie-modal.hidden {\n  display: none !important;\n}\n\n#cookie-modal {\n  background: #fff;\n  color: #17497a;\n  border: 1.5px solid #c7daf5;\n  border-radius: 18px;\n  box-shadow: 0 8px 32px rgba(23, 73, 122, 0.12), 0 1.5px 8px rgba(23,73,122,0.10);\n  padding: 32px 22px 22px 22px;\n  max-width: 350px;\n  width: 92vw;\n  display: flex !important;\n  flex-direction: column;\n  align-items: center;\n  text-align: center;\n  gap: 20px;\n  font-size: 1rem;\n  font-family: inherit;\n  animation: fadeInCookie 0.7s cubic-bezier(.4,1.2,.4,1) both;\n  position: fixed !important;\n  top: 50% !important;\n  left: 50% !important;\n  transform: translate(-50%, -50%) !important;\n  z-index: 9999 !important;\n}\n\n#cookie-modal .cookie-text {\n  color:var(--light-text);\n  font-size: 1.08rem;\n  margin-bottom: 0;\n}\n\n#cookie-modal .cookie-modal-buttons {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 18px;\n  width: 100%;\n  margin-top: 8px;\n}\n#cookie-modal .cookie-modal-buttons .btn {\n  flex: 1 1 0;\n  min-width: 100px;\n  max-width: 160px;\n  width: 100%;\n  padding: 12px 0;\n  font-size: 1rem;\n  border-radius: 22px;\n  border: none;\n  cursor: pointer;\n  font-weight: 600;\n  transition: box-shadow 0.18s, background 0.18s, color 0.18s;\n  box-shadow: 0 2px 8px rgba(23,73,122,0.08);\n  outline: none;\n}\n#cookie-modal .cookie-modal-buttons .btn-accept {\n  background: #eaf0f8;\n  color: #17497a;\n}\n#cookie-modal .cookie-modal-buttons .btn-decline {\n  background: #eaf0f8;\n  color: #17497a;\n}\n#cookie-modal .cookie-modal-buttons .btn-accept:hover {\n  background: #d8e4f7;\n  color: #17497a;\n  box-shadow: 0 4px 16px rgba(23,73,122,0.11);\n}\n#cookie-modal .cookie-modal-buttons .btn-decline:hover {\n  background: #d8e4f7;\n  color: #17497a;\n  box-shadow: 0 4px 16px rgba(23,73,122,0.11);\n}\n@media (max-width: 500px) {\n  #cookie-modal {\n    padding: 18px 6vw 18px 6vw;\n    max-width: 98vw;\n  }\n  #cookie-modal .cookie-modal-buttons {\n    flex-direction: column;\n    gap: 10px;\n  }\n  #cookie-modal .cookie-modal-buttons .btn {\n    max-width: 100%;\n  }\n}\n\n@keyframes fadeInCookie {\n  0% {\n    opacity: 0;\n    transform: translate(-50%, -60%) scale(0.97);\n  }\n  100% {\n    opacity: 1;\n    transform: translate(-50%, -50%) scale(1);\n  }\n}\n\n#cookie-overlay.hidden {\n  display: none !important;\n}\n\n#cookie-modal, #cookie-overlay {\n  pointer-events: auto;\n}\nbody {\n  overflow-x: hidden;\n}\n\n/* DEBUG : forcer le centrage et la visibilité */\n#cookie-modal, .cookie-modal {\n  position: fixed !important;\n  top: 50% !important;\n  left: 50% !important;\n  transform: translate(-50%, -50%) !important;\n  z-index: 9999 !important;\n  background: var(--primary-color, #17497a);\n  color: var(--light-text, #fff);\n  border: none;\n  align-items: center;\n  text-align: center;\n  gap: 22px;\n  font-size: 1rem;\n  font-family: inherit;\n}"
