            "text": "@import url('variables.css');\n\n/*--------------------*/\n/* CONTACT - BANNIERE */\n/*--------------------*/\n\n.contact-banner {\n  background-color: var(--primary-color);\n  color: var(--light-text);\n  padding: var(--spacing-lg) 0;\n  position: relative;\n  overflow: hidden;\n}\n\n.contact-banner::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-image: linear-gradient(135deg, rgba(10, 72, 132, 0.9) 0%, rgba(10, 72, 132, 0.7) 100%);\n  z-index: 1;\n}\n\n.contact-banner-content {\n  position: relative;\n  z-index: 2;\n  text-align: center;\n}\n\n.contact-banner h1 {\n  font-size: 3rem;\n  margin-bottom: var(--spacing-sm);\n}\n\n.contact-banner p {\n  font-size: 1.1rem;\n  max-width: 800px;\n  margin: 0 auto;\n}\n\n.contact-section {\n  padding-top: 40px;\n  padding-right: 40px;\n  padding-left: 40px;\n  padding-bottom: 0;\n}\n\n.contact-container {\n  display: grid;\n  grid-template-columns: 1fr 1fr;\n  gap: var(--spacing-lg);\n}\n\n\n/*----------------------*/\n/* CONTACT - FORMULAIRE */\n/*----------------------*/\n\n.contact-form {\n  background-color: var(--light-text);\n  padding: var(--spacing-md);\n  border-radius: var(--border-radius-md);\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);\n}\n\n.contact-form h2 {\n  margin-bottom: var(--spacing-md);\n}\n\n.form-group {\n  margin-bottom: var(--spacing-sm);\n}\n\n.form-group label {\n  display: block;\n  margin-bottom: 5px;\n  font-weight: 500;\n}\n\n.form-control {\n  width: 100%;\n  padding: 10px 15px;\n  border: 1px solid #ddd;\n  border-radius: var(--border-radius-sm);\n  font-family: var(--main-font);\n  font-size: var(--normal-text);\n  transition: border-color var(--transition-fast);\n}\n\n.form-control:focus {\n  outline: none;\n  border-color: var(--primary-color);\n  box-shadow: 0 0 0 3px rgba(10, 72, 132, 0.1);\n}\n\n.form-control.error {\n  border-color: var(--secondary-color);\n}\n\ntextarea.form-control {\n  min-height: 150px;\n  resize: vertical;\n}\n\n.error-message {\n  color: var(--secondary-color);\n  font-size: var(--small-text);\n  margin-top: 5px;\n}\n\n.form-submit {\n  margin-top: var(--spacing-md);\n}\n\n\n/*------------------------*/\n/* CONTACT - INFORMATIONS */\n/*------------------------*/\n\n.contact-info {\n  display: flex;\n  flex-direction: column;\n  padding: var(--spacing-md);\n  position: relative;\n}\n\n.contact-info-logo {\n  width: 270px;\n  height: 270px;\n  margin: var(--spacing-xs) auto;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.contact-info-logo img {\n  width: 100%;\n  height: auto;\n  object-fit: contain;\n  display: block;\n}\n\n.contact-info h2 {\n  margin-bottom: var(--spacing-md);\n  position: relative;\n  z-index: 2;\n}\n\n.contact-info p {\n  margin-bottom: var(--spacing-md);\n}\n\n.contact-details {\n  margin-bottom: var(--spacing-md);\n}\n\n.contact-item {\n  display: flex;\n  align-items: flex-start;\n  margin-bottom: var(--spacing-sm);\n}\n\n.contact-item i {\n  color: var(--primary-color);\n  font-size: 1.2rem;\n  margin-right: 15px;\n  margin-top: 5px;\n  width: 20px;\n  text-align: center;\n}\n\n.contact-item-content h4 {\n  margin-bottom: 5px;\n  font-size: 1.1rem;\n}\n\n.contact-item-content p {\n  margin-bottom: 0;\n  color: var(--dark-gray);\n}\n\n\n/*------------------*/\n/* CONTACT - SOCIAL */\n/*------------------*/\n\n.contact-social {\n  display: flex;\n  justify-content: center;\n  gap: 15px;\n  position: relative;\n  z-index: 10;\n  overflow: visible !important;\n}\n\n.contact-social a {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  position: relative;\n  width: 40px;\n  height: 40px;\n  background-color: var(--primary-color);\n  border-radius: 50%;\n  color: var(--light-text);\n  font-size: 1.2rem;\n  transition: all var(--transition-normal);\n}\n\n.contact-social a.svg-icon {\n  padding: 6px;\n  overflow: visible !important;\n}\n\n.contact-social a.svg-icon img {\n  width: 100%;\n  height: 100%;\n  object-fit: contain;\n  filter: brightness(0) saturate(100%) invert(100%) sepia(0%) saturate(0%) hue-rotate(0deg) brightness(100%) contrast(100%);\n  transition: all var(--transition-normal);\n}\n\n.contact-social-label {\n  opacity: 0;\n  pointer-events: none;\n  position: absolute;\n  left: 50%;\n  top: 120%;\n  transform: translateX(-50%);\n  background: var(--primary-color);\n  color: var(--light-text);\n  font-size: 0.75em;\n  padding: 3px 8px;\n  border-radius: 3px;\n  white-space: nowrap;\n  transition: all 0.3s ease;\n  z-index: 1000;\n  box-shadow: 0 2px 6px rgba(0,0,0,0.3);\n  font-weight: normal;\n}\n\n.contact-social-label::before {\n  content: '';\n  position: absolute;\n  top: -4px;\n  left: 50%;\n  transform: translateX(-50%);\n  border-left: 4px solid transparent;\n  border-right: 4px solid transparent;\n  border-bottom: 4px solid var(--primary-color);\n}\n\n.contact-social a:hover .contact-social-label,\n.contact-social a:focus .contact-social-label {\n  opacity: 1;\n  transform: translateX(-50%) translateY(0);\n  visibility: visible;\n}\n\n.contact-social a:hover {\n  background-color: var(--light-text);\n  color: var(--primary-color);\n  transform: translateY(-3px);\n}\n\n.contact-social a.svg-icon:hover img {\n  filter: brightness(0) saturate(100%) invert(25%) sepia(90%) saturate(1000%) hue-rotate(190deg) brightness(90%) contrast(95%);\n}\n\n.contact-social a.svg-icon {\n  padding: 6px;\n  overflow: hidden;\n  background-color: var(--light-gray);\n}\n\n.contact-social a.svg-icon img {\n  width: 100%;\n  height: 100%;\n  object-fit: contain;\n  transition: all var(--transition-normal);\n  filter: brightness(0) saturate(100%) invert(25%) sepia(90%) saturate(1000%) hue-rotate(190deg) brightness(90%) contrast(95%);\n}\n\n.contact-social a:hover {\n  background-color: var(--primary-color);\n  color: var(--light-text);\n  transform: translateY(-3px);\n}\n\n.contact-social a.svg-icon:hover {\n  background-color: var(--primary-color);\n}\n\n.contact-social a.svg-icon:hover img {\n  filter: brightness(0) saturate(100%) invert(100%) sepia(0%) saturate(0%) hue-rotate(0deg) brightness(100%) contrast(100%);\n}\n\n\n/*----------------------*/\n/* CONTACT - GOOGLE MAP */\n/*----------------------*/\n\n#map {\n  width: 100%;\n  height: 100%;\n}\n\n.map-section {\n  margin-top: var(--spacing-lg);\n}\n\n.map-container {\n  height: 400px;\n  border-radius: var(--border-radius-md);\n  padding-bottom: 40px;\n  overflow: hidden;\n  box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);\n}\n\n\n\n\n/*------------------------*/\n/* CONTACT - NOTIFICATION */\n/*------------------------*/\n\n.contact-notif {\n  position: fixed;\n  bottom: 20px;\n  left: 50%;\n  transform: translateX(-50%);\n  background: var(--light-text);\n  color: var(--dark-gray);\n  padding: 12px 25px;\n  border-radius: var(--border-radius-sm);\n  box-shadow: 5px 15px rgba(0,0,0,0.15);\n  z-index: 9999;\n  font-weight: 500;\n  animation: fadeInOut 4s ease forwards;\n}\n.contact-notif.success { border-left: 5px solid var(--primary-color); }\n.contact-notif.error { border-left: 5px solid var(--secondary-color); }\n\n@keyframes fadeInOut {\n  0%   { opacity: 0; transform: translateX(-50%) translateY(10px); }\n  10%  { opacity: 1; transform: translateX(-50%) translateY(0); }\n  90%  { opacity: 1; }\n  100% { opacity: 0; transform: translateX(-50%) translateY(10px); }\n}\n\n\n/* Responsive */\n@media (max-width:1000px) {\n  .contact-container {\n    grid-template-columns: 1fr;\n  }\n}\n\n@media (max-width: 768px) {\n  .contact-container {\n    grid-template-columns: 1fr;\n  }\n\n  .contact-info {\n    order: 2;\n    margin-top: var(--spacing-md);\n  }\n\n  .contact-form {\n    order: 1;\n  }\n}\n\n\n@media (max-width: 470px) {\n  .contact-section {\n    padding: 10px 0;\n  }\n  \n  .contact-form,\n  .contact-info {\n    padding: var(--spacing-sm);\n  }\n}\n\n@media (max-width: 392px) {\n  .contact-banner h1 {\n    font-size: 2.1rem;\n    margin-bottom: var(--spacing-xs);\n  }\n\n  .contact-form {\n    padding: 15px 10px;\n    width: 100%;\n    box-sizing: border-box;\n    margin: 0 auto;\n  }\n  \n  .form-control {\n    padding: 8px 10px;\n  }\n  \n  .contact-section {\n    padding: 10px;\n  }\n  \n  .container {\n    padding-left: 5px;\n    padding-right: 5px;\n  }\n}\n\n@media (max-width: 348px) {\n  .contact-form,\n  .contact-info {\n    padding: var(--spacing-xs);\n  }\n}\n"
